{
  "name": "ssVault",
  "version": "1.0.0",
  "description": "Secure local password and secrets manager built with Electron.",
  "productName": "ssVault",
  "main": "src/main/main.js",
  "scripts": {
    "test": "echo \"No automated tests configured\"",
    "start": "electron-forge start",
    "build": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron-forge/publisher-s3": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "electron": "^38.0.0"
  },
  "build": {
    "appId": "pl.edu.wszib.mromasze.ssvault",
    "productName": "ssVault",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "@journeyapps/sqlcipher": "^5.3.1",
    "adm-zip": "^0.5.16",
    "bcrypt": "^6.0.0",
    "boostrap": "^2.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "openpgp": "^6.2.2",
    "os": "^0.1.2",
    "path": "^0.12.7"
  }
}
